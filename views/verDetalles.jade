extends layout

block extraHeader
    <script src="/js/jquery-2.1.3.min.js"></script>

block content
    .container-fluid
        .row
            .col-xs-12.col-sm-12.col-md-8.center.fondo
                h1 Factura número #{factura}
                br
                table(align="center" border="1")
                
                    tr
                        td  Lugar:
                        td  Precio:
                    
                    each detalle, i in detalles
                        tr
                            td !{JSON.parse(JSON.stringify(detalle)).LUGAR}
                            td !{JSON.parse(JSON.stringify(detalle)).PRECIO}
                br
                    
                a(href='/') Cerrar sesión
